		<icon height="180" src="images/ios/180.png" width="180" />
		<icon height="196" src="images/ios/196.png" width="196" />
		<icon height="1024" src="images/ios/1024.png" width="1024" />

		<splash src="images/ios/Default@1x~universal~anyany.png" />
		<splash src="images/ios/Default@2x~universal~anyany.png" />
		<splash src="images/ios/Default@3x~universal~anyany.png" />

		<resource-file src="GoogleService-Info.plist" />

		<edit-config target="BGTaskSchedulerPermittedIdentifiers" file="*-Info.plist" mode="merge" overwrite="true">
			<array>
				<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
			</array>
		</edit-config>
		<edit-config target="NSLocationWhenInUseUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Location access is used to show events, people, and locations in your vicinity.</string>
		</edit-config>
		<edit-config target="NSLocationAlwaysAndWhenInUseUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Location access is used to show events, people, and locations in your vicinity.</string>
		</edit-config>
		<edit-config target="NSLocationAlwaysUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Location access is used to show events, people, and locations in your vicinity.</string>
		</edit-config>
		<edit-config target="NSPhotoLibraryUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Photo library access allows you to select pictures for chats or your profile settings.</string>
		</edit-config>
		<edit-config target="NSCameraUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Access to camera allows you to take new pictures for chats, your profile settings and make video calls to other people.</string>
		</edit-config>
		<edit-config target="NSMicrophoneUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Access to microphone is used to make video calls to other people.</string>
		</edit-config>
		<edit-config target="NSBluetoothPeripheralUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Bluetooth is used for Bluetooth Matching of Buddies in your immediate surrounding. You may turn it off in your settings.</string>
		</edit-config>
		<edit-config target="NSBluetoothAlwaysUsageDescription" file="*-Info.plist" mode="merge" overwrite="true">
			<string>Bluetooth is used for Bluetooth Matching of Buddies in your immediate surrounding. You may turn it off in your settings.</string>
		</edit-config>
		<edit-config target="CBAdvertisementDataServiceUUIDsKey" file="*-Info.plist" mode="merge" overwrite="true">
			<array>
				<string>e8a98f86-f973-442b-a099-b0b2d2c4f20e</string>
				<string>1e6153b0-9c7e-47e9-a14a-c917fad22e41</string>
			</array>
		</edit-config>
		<edit-config target="CBAdvertisementDataOverflowServiceUUIDsKey" file="*-Info.plist" mode="merge" overwrite="true">
