				<string>1e6153b0-9c7e-47e9-a14a-c917fad22e41</string>
			</array>
		</edit-config>
	</platform>
	<platform name="android">
		<allow-intent href="market:*" />

		<preference name="AndroidLaunchMode" value="singleTop" />
		<preference name="android-minSdkVersion" value="33" />
		<preference name="android-targetSdkVersion" value="33" />
		<preference name="AndroidLaunchMode" value="singleInstance" />
		<preference name="loadUrlTimeoutValue" value="700000" />
		<preference name="AndroidXEnabled" value="true" />
		<preference name="AndroidPersistentFileLocation" value="Compatibility" />
		<preference name="GradlePluginKotlinVersion" value="1.6.20" />

		<config-file parent="/manifest" target="AndroidManifest.xml">
			<uses-permission android:name="android.permission.CAMERA" />
			<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
			<uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL" />
			<uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
			<uses-permission android:name="android.permission.RECORD_AUDIO" />
			<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
			<queries>
				<intent>
					<action android:name="android.support.customtabs.action.CustomTabsService" />
				</intent>
			</queries>
		</config-file>
		<config-file parent="/manifest/application" target="AndroidManifest.xml">
			<uses-library android:name="org.apache.http.legacy" android:required="false" />
		</config-file>

		<icon density="ldpi" foreground="images/android/36.png" background="images/android/36.png" />
		<icon density="mdpi" foreground="images/android/48.png" background="images/android/48.png" />
		<icon density="hdpi" foreground="images/android/72.png" background="images/android/72.png" />
		<icon density="xhdpi" foreground="images/android/96.png" background="images/android/96.png" />
		<icon density="xxhdpi" foreground="images/android/144.png" background="images/android/144.png" />
		<icon density="xxxhdpi" foreground="images/android/192.png" background="images/android/192.png" />

		<resource-file src="google-services.json" target="app/google-services.json" />
	</platform>
